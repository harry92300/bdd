exo 1
select nom, annéeNaiss from artiste where annéeNaiss<1950
exo 2
SELECT titre FROM film WHERE genre = 'Drame';
exo 3
SELECT nomRôle FROM role WHERE idActeur = 'Bruce Willis';
exo 4
select idRéalisateur from film where idfilm = 'Memento'
exo 5
select note, titre from notation natural join film where titre = 'Fargo'
exo 6
SELECT nom, prénom, nomrôle FROM artiste NATURAL JOIN role WHERE nomrôle = 'Chewbacca';
